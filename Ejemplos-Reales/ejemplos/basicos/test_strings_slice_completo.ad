; Test slicing completo: todos los casos posibles
let s = "abcdefghij"

; Slice desde inicio
let s1 = s[0:3]    ; "abc"
let s2 = s[0:5]    ; "abcde"

; Slice desde medio
let s3 = s[3:6]    ; "def"
let s4 = s[5:8]    ; "fgh"

; Slice hasta fin
let s5 = s[7:10]   ; "hij"
let s6 = s[9:10]   ; "j"

print s
print s1
print s2
print s3
print s4
print s5
print s6

