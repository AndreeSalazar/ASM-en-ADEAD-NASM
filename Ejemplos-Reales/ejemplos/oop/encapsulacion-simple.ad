struct Banco {
    saldo: int64,
    pub nombre: string
}

print "Creando banco..."
let mut banco = Banco {
    nombre: "Mi Banco",
    saldo: 0
}
print "Banco creado"

print "Accediendo a campo público (nombre)..."
let nombre_banco = banco.nombre
print nombre_banco

print "Ejemplo de encapsulación (solo campos) completado"

