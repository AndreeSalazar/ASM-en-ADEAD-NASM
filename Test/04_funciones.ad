# Test 04: Funciones
# Funciones con recursi√≥n

fn factorial(n) {
    if n <= 1 {
        return 1
    }
    return n * factorial(n - 1)
}

fn fibonacci(n) {
    if n <= 1 {
        return n
    }
    return fibonacci(n - 1) + fibonacci(n - 2)
}

fn suma(a, b) {
    return a + b
}

# Probar funciones
print factorial(5)
print fibonacci(10)
print suma(3, 7)

