# Test 06: Librería Estándar
# Funciones matemáticas built-in

# min y max
print min(5, 3)
print max(5, 3)

# Valor absoluto
print abs(-10)

# Potencia
print pow(2, 8)

# Clamp (limitar rango)
print clamp(15, 0, 10)
print clamp(-5, 0, 10)
print clamp(5, 0, 10)

# Signo
print sign(-42)
print sign(0)
print sign(42)

# GCD y LCM
print gcd(48, 18)
print lcm(4, 6)

# Factorial
print factorial(6)

# Par/Impar
print is_even(4)
print is_odd(7)

