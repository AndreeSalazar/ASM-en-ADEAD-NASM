; Test completo: Todas las operaciones de strings
let s1 = "hola"
let s2 = "mundo"
let s3 = s1 + " " + s2    ; Concatenación múltiple
print s3

let slice1 = s3[0:4]      ; "hola"
print slice1

let upper = s3.upper()    ; "HOLA MUNDO"
print upper

let lower = upper.lower() ; "hola mundo"
print lower

let length = len(s3)      ; 10
print length

