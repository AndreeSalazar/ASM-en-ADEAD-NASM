# TEST 07: Constructor con Parámetros
# Objetivo: Verificar fn new(param1, param2)

print "=== TEST 07: Constructor con Params ==="

struct Rectangulo {
    ancho
    alto
}

# Factory con parámetros
fn crear_rectangulo(w, h) {
    return Rectangulo { ancho: w, alto: h }
}

# Crear con valores específicos
let rect1 = crear_rectangulo(10, 5)
let rect2 = crear_rectangulo(20, 15)

print "Rectangulo 1 creado"
print "Ancho:"
print rect1.ancho
print "Alto:"
print rect1.alto

print "Rectangulo 2 creado"
print "Ancho:"
print rect2.ancho
print "Alto:"
print rect2.alto

print "TEST 07: PASADO"

