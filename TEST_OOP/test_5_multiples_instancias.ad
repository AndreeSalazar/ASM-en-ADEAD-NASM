# Test 5: Múltiples Instancias Independientes
# Objetivo: Verificar que cada instancia es independiente

struct Punto {
    x
    y
}

fn Punto_mover(self, dx, dy) {
    self.x = self.x + dx
    self.y = self.y + dy
}

let p1 = Punto { x: 10, y: 20 }
let p2 = Punto { x: 100, y: 200 }

p1.mover(5, 5)
p2.mover(-10, -10)

print p1.x
print p1.y
print p2.x
print p2.y

