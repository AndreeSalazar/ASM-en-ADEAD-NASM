# Objetivo: Verificar independencia de datos entre instancias

print "=== TEST 05: Multiples Instancias ==="

struct Punto {
    x
    y
}

# Crear varias instancias
let p1 = Punto { x: 10, y: 20 }
let p2 = Punto { x: 30, y: 40 }
let p3 = Punto { x: 50, y: 60 }

print "3 puntos creados"

# Verificar que son independientes
print "Punto 1 - x:"
print p1.x

print "Punto 2 - x:"
print p2.x

print "Punto 3 - x:"
print p3.x

# Los valores deben ser diferentes
print "TEST 05: PASADO"

