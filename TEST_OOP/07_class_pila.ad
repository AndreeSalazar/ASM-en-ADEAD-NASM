# Test 07: Clase Pila (Stack)
# Estructura de datos con array interno

class Pila {
    fn new() {
        self.items = []
    }
    
    fn push(self, item) {
        self.items.append(item)
    }
    
    fn pop(self) {
        return self.items.pop()
    }
    
    fn size(self) {
        return len(self.items)
    }
    
    fn is_empty(self) {
        if len(self.items) == 0 {
            return 1
        }
        return 0
    }
}

# Crear pila
let pila = Pila.new()
print pila.is_empty()
print pila.size()

# Agregar elementos
pila.push(10)
pila.push(20)
pila.push(30)
print pila.size()

# Sacar elementos
print pila.pop()
print pila.pop()
print pila.size()

# Salida esperada:
